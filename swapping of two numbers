#include<stdio.h> 
void swap (int* a, int* b );
int main ()
{
int num1, num2,;
printf(" enter two no. :");
scanf("%d%d", &num1, &num2);
printf(" before swapping.....value of num1=%d, num2=%d\n", num1, num2);
swap(&num1, &num2);
printf(" after swapping.....value of num1=%d, num2=%d\n", num1, num2);
 return 0;
}
void swap ( int *a, int *b)  // a=& num1, b=&num2
{
int temp;
temp= *a;
*a=*b;
*b = temp;
printf("swapped numbers : %d and %d\n", *a, *b);
}
