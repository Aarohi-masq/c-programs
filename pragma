#include<stdio.h>
#pragma pack(10
struct sample
{
int i1;
char c1;
int i2;
char c2;
float f1;
};
int main()
{
struct sample s1{10, 'a', 20, 'b', 3.14};
printf("size of structure: %d\n\n", sizeof(s1));

printf("addderess of int-1:%p\n", &s1.i1);
printf("adderess of char-1: %p\n", &s1.c1);
printf("adderess of int2: %p\n", &s1.i2);
printf ("adderess of char2: %p\n", &s1.c2);
printf("adderesss of float1: %p\n", &s1.f1);
return 0;
}
