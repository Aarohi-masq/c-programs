#include<stdio.h>
 int main()
{ 
int a[10], i;                                                                         // defn....10*4b= 40b block 
printf("base adderess: %p\n", a);
printf("base adderess: %p\n", a+1);
for (i=0; i<10; i++);
{
scanf("%d", &a[i]);
}
for (i=0; i<10; i++);
{
printf("%d", a[i]);
}
return 0;
}
