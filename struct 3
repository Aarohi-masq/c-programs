#include <string.h>

struct student
{
  int id;
  float percentage;
};
void main()
{
  int i;
  struct student record[3];
  clrscr();
  for(i=0; i<3;i++)
  {
    scanf("%d", &record[i].id);
    scanf("%d", &record[i]. percentage);
  }
  for(i=0; i<3; i++)
  {
    printf("record of student :%d\n ", i+1);
    printf("id is:%d\n", record[i].id);
    printf("percentage is:%f\n\n", record[i].percentage);
  }
  return 0;
  
}
